    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-typescript": "~4.0.0",
    "rollup": "~1.16.6",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-terser": "~5.1.1",
